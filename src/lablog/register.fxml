         <GridPane.margin>
            <Insets top="-5.0" />
         </GridPane.margin></TextField>
      <Button fx:id="btnRegister" mnemonicParsing="false" prefHeight="27.0" prefWidth="185.0" text="Register" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="10">
         <GridPane.margin>
