      <Label prefHeight="41.0" prefWidth="195.0" text="Student Registration" GridPane.columnSpan="2" GridPane.halignment="RIGHT">
         <font>
            <Font size="20.0" />
         </font>
         <GridPane.margin>
            <Insets bottom="20.0" right="5.0" />
         </GridPane.margin></Label>
      <Label text="First Name:" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets />
