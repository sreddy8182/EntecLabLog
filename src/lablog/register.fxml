<GridPane alignment="center" hgap="10" prefHeight="415.0" prefWidth="335.0" vgap="10" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
